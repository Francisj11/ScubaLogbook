<p id="notice"><%= notice %></p>

<h1>Listing Dives</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Time</th>
      <th>Duration</th>
      <th>Depth</th>
      <th>Dive type</th>
      <th>Dive buddy</th>
      <th>Location</th>
      <th>Rating</th>
      <th>Image url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dives.each do |dife| %>
      <tr>
        <td><%= dife.date %></td>
        <td><%= dife.time %></td>
        <td><%= dife.duration %></td>
        <td><%= dife.depth %></td>
        <td><%= dife.dive_type %></td>
        <td><%= dife.dive_buddy %></td>
        <td><%= dife.location %></td>
        <td><%= dife.rating %></td>
        <td><%= dife.image_URL %></td>
        <td><%= link_to 'Show', dife %></td>
        <td><%= link_to 'Edit', edit_dife_path(dife) %></td>
        <td><%= link_to 'Destroy', dife, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dife', new_dife_path %>
